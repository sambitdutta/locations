<section class="jumbotron text-center">
    <div class="container">
        <h1 class="jumbotron-heading">Maps</h1>
        <p class="lead text-muted"></p>
        <h2><%= params[:username] %></h2>
    </div>
</section>

<div class="album text-muted">
    <div class="container">

        <div class="row">
            
            <%= render partial: 'locations/location', collection: @dashboard.shared_by_me(true) %>
            
        </div>

    </div>
</div>


<% content_for :js do %>
  
  <%= javascript_include_tag 'holder.min' %>

  <script>
    $(function () {
        Holder.addTheme("thumb", {background: "#55595c", foreground: "#eceeef", text: "Thumbnail"});
    });
  </script>
<%end%>